// config of USBM
// -------------------------------------------------------------------------------

; schratchpad
		.SCR	0x780, 256
      
; 2k instructions
CODESIZE	.EQU	2048

		.END	CODESIZE - 1

_INT_		.EQU	0
_I2C_		.EQU	1
_SPI_		.EQU	0
_UART_	  .EQU	1

_FMC_		.EQU	0
_BUG_		.EQU	1

